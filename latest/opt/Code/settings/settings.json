{
    "workbench.startupEditor": "none",
    "window.menuBarVisibility": "toggle",
    "git.autofetch": true,
    "editor.rulers": [],
    "files.autoSave": "onFocusChange",
    "csharpextensions.privateMemberPrefix": "_",
    "csharpextensions.useThisForCtorAssignments": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "C_Cpp.default.compilerPath": "/usr/bin/gcc-12",
    "markdown.extension.list.indentationSize": "inherit",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "C_Cpp.vcFormat.indent.preserveWithinParentheses": true,
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
    "C_Cpp.formatting": "vcFormat",
    "workbench.colorTheme": "alliebeans-darker",
    "cmake.showOptionsMovedNotification": false,
    "git.openRepositoryInParentFolders": "never",
    "C_Cpp.vcFormat.space.pointerReferenceAlignment": "right",
    "window.zoomLevel": 1,
    "C_Cpp.vcFormat.space.withinInitializerListBraces": false,
    "cmake.configureOnOpen": true,
    "editor.accessibilitySupport": "off",
    "C_Cpp.vcFormat.indent.preserveComments": true,
    "editor.detectIndentation": false,
    "editor.lightbulb.enabled": "onCode",
    "editor.roundedSelection": false,
    "telemetry.telemetryLevel": "off"
}